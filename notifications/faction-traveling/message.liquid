{
	"embeds": [
		{
			"title": "Flying Members of {{ faction_name }}",
			{% if flying_members.size == 0 %}
			"description": "No members are flying.",
			{% endif %}
			"fields": [
				{% for destination in flying_members %}
				{
					"name": "{{ destination[0] }}",
					"value": "{% for member in destination[1] %}[{{ member[1].username }}](https://torn.com/profiles.php?XID={{ member[1].id }}) - <t:{{ member[1].timestamp }}:R>\n{% endfor %}"
				}{% unless forloop.last %},{% endunless %}
				{% endfor %}
			]
		}
	]
}
