{
	"embeds": [
		{
			"title": "Flying Members of {{ faction_name }}",
			{% if flying_members.size == 0 %}
			"description": "No members are flying."
			{% else %}
			"fields": [
				{% for destination in flying_members %}
				{
					"name": "{{ destination[0] }}",
				"value": "{% for member in destination[1] %}[{{ member[1].username }}](https://torn.com/profiles.php?XID={{ member[1].tid }}) - {% if member[1].timestamp %}<t:{{ member[1].timestamp }}:R>{% else %}Unknown{% endif %}\n{% endfor %}"
				}{% unless forloop.last %},{% endunless %}
				{% endfor %}
			]
			{% endif %}
		},
		{
			"title": "Hospitalized Members of {{ faction_name }}",
			{% if hospital_members.size == 0 %}
			"description": "No members are hospitalized overseas."
			{% else %}
			"fields": [
				{% for destination in hospital_members %}
				{
					"name": "{{ destination[0] }}",
					"value": "{% for member in destination[1] %}[{{ member[1].username }}](https://torn.com/profiles.php?XID={{ member[1].tid }}) - <t:{{ member[1].hospital_end_time }}:R>\n{% endfor %}"
				}{% unless forloop.last %},{% endunless %}
				{% endfor %}
			]
			{% endif %}
		},
		{
			"title": "Abroad Members of {{ faction_name }}",
			{% if abroad_members.size == 0 %}
			"description": "No members are abroad."
			{% else %}
			"fields": [
				{% for destination in abroad_members %}
				{
					"name": "{{ destination[0] }}",
					"value": "{% for member in destination[1] %}[{{ member[1].username }}](https://torn.com/profiles.php?XID={{ member[1].tid }})\n{% endfor %}"
				}{% unless forloop.last %},{% endunless %}
				{% endfor %}
			]
			{% endif %}
		}
	]
}
